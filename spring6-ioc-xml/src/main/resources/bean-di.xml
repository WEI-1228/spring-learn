<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="book" class="cn.anlper.iocxml.di.Book">
        <property name="author" value="Anlper"></property>
        <property name="bname" value="前端开发"></property>
    </bean>

    <bean id="book1" class="cn.anlper.iocxml.di.Book">
        <constructor-arg name="author" value="Anlper">
        </constructor-arg>
        <constructor-arg name="bname" value="Java开发"></constructor-arg>
    </bean>
</beans>